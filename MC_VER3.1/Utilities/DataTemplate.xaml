<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MC_VER3.ViewModel"
                    xmlns:view="clr-namespace:MC_VER3.View">
    
    <DataTemplate DataType="{x:Type vm:TasksVM}">
        <view:Tasks />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TasksImportantVM}">
        <view:TasksImportant />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NotesVM}">
        <view:Notes />
    </DataTemplate>
    
</ResourceDictionary>